<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body class="container">

    <header>
        <%- include('../partials/header', {exampleTitle: exampleTitle}); %>
    </header>

    <main>
        <div class="jumbotron">
            <h1>This is great</h1>
            <p>Welcome to templating using EJS</p>

            <h2>Variable</h2>
            <p>
                <%= exampleTeks %>
            </p>

            <ul>
                <% exampleJson.forEach(element => { %>
                    <li>
                        <strong>
                            <%= element.name %>
                        </strong>
                        representing <%= element.organization %>,
                            born <%= element.birth_year %>
                    </li>
                <% }) %>
            </ul>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>

</html>